decl
	int gcd(int a, int b);
enddecl

int gcd(int a, int b){
decl
	int ret;
enddecl

begin
	if(a == 0) then
		ret = b;
	else
		ret = gcd((b%a), a);
	endif;

	return ret;
end	
}

